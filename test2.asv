W_opt = precoding_bench_mark(H, settings, 4);
for k = 1:settings.num_of_Beams
  [U,S] = schur(W_opt(:,:,k));
  v_s = sqrt(1/2) * (randn(settings.num_of_Antenna , 1) + 1j * randn(settings.num_of_Antenna , 1));
  w_k = U * sqrtm(S) * v_s;
  v_k = w_k / norm(w_k);
  V = [V,v_k];
end

